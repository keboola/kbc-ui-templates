{
   "baseUrl": "https://api.surveymonkey.net/v3/",
   "authentication": {
    "type": "query",
    "query": {
     "api_key": {
      "attr": "api_key"
     }
    }
   },
   "pagination": {
    "method": "response.url",
    "urlKey": "links.next",
    "limitStop": {
     "field": "total"
    }
   },
   "http": {
            "headers": {
                "authorization": {
                    "function": "concat",
                    "args": [
                        "bearer ",
                        {
                            "attr": "#access_token"
                        }
                    ]
                },
                "Content-Type": "application/json"
            }
        }
}
