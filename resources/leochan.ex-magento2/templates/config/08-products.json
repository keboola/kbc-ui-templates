{
    "name":"Products",
    "description":"Retrieve a list of products.",
    "data":{
        "incrementalOutput": true,
        "debug":true,
        "jobs":[
            {
                "endpoint":"products",
                "dataType":"products",
                "dataField":"items"
            }
        ],
        "mappings":{
            "products":{
                "id":{
                    "mapping":{
                        "destination":"id",
                        "primaryKey":true
                    }
                },
                "sku":{
                    "mapping":{
                        "destination":"sku",
                        "primaryKey":true   
                    }
                },
                "name":{
                    "type":"column",
                    "mapping":{"destination":"name"}
                },
                "attribute_set_id":{
                    "type":"column",
                    "mapping":{"destination":"attribute_set_id"}
                },
                "price":{
                    "type":"column",
                    "mapping":{"destination":"price"}
                },
                "status":{
                    "type":"column",
                    "mapping":{"destination":"status"}
                },
                "visibility":{
                    "type":"column",
                    "mapping":{"destination":"visbility"}
                },
                "type_id":{
                    "type":"column",
                    "mapping":{"destination":"type_id"}
                },
                "created_at":{
                    "type":"column",
                    "mapping":{"destination":"created_at"}
                },
                "updated_at":{
                    "type":"column",
                    "mapping":{"destination":"updated_at"}
                }
            }
        }
    }
}