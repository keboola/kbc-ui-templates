{
  "name": "Videos",
  "description": "List of videos in my channels",
  "data": {
    "jobs": [
      {
        "endpoint": "channels?mine=true&part=id,snippet",
        "dataType": "channels",
        "dataField": "items",
        "children": [
          {
            "endpoint": "search?type=video&channelId={channelId}&part=id",
            "dataField": "items",
            "placeholders": {
              "channelId": "id"
            },
            "children": [
              {
                "endpoint": "videos?id={videoId}&part=id,snippet,statistics",
                "dataType": "videosDetail",
                "dataField": "items",
                "placeholders": {
                  "videoId": "id.videoId"
                }
              }
            ]
          }
        ]
      }
    ]
  }

}
