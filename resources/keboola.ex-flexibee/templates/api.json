{
  "baseUrl": {
    "function": "concat",
    "args": [
     "https://",
     {
      "attr": "host"
     },
     ".flexibee.eu/c/",
      {
        "attr": "domain"
      },
      "/"
    ]
  },
  "authentication": {
    "type": "basic"
  },
  "http": {
    "defaultOptions": {
      "headers": {
        "Accept": "application/json"
      }
    }
  },
  "pagination": {
    "method": "offset",
    "offsetParam": "start",
    "limit": "100",
    "limitParam": "limit"
  }
}
