{
  "name": "Full Mode - always get everything",
  "description": "Download from GitHub\n\n - Your Organizations\n\n - Org Members\n - Org Teams\n - Org Repositories\n - Repo Issues\n - Repo Commits",
  "data": {
    "incrementalOutput": false,
    "jobs": [
      {
        "endpoint": "user/orgs",
        "dataType": "organizations",
        "children": [
          {
            "endpoint": "orgs/{1:login}/members",
            "dataType": "members",
            "placeholders": {
              "1:login": "login"
            }
          },
          {
            "endpoint": "orgs/{1:login}/teams",
            "dataType": "teams",
            "placeholders": {
              "1:login": "login"
            }
          },
          {
            "endpoint": "orgs/{1:login}/repos",
            "dataType": "repos",
            "placeholders": {
              "1:login": "login"
            },
            "children": [
              {
                "endpoint": "repos/{2:login}/{1:name}/commits",
                "recursionFilter": "size>=1",
                "placeholders": {
                  "1:name": "name",
                  "2:login": "login"
                },
                "dataType": "commits"

              },
              {
                "endpoint": "repos/{2:login}/{1:name}/issues",
                "placeholders": {
                  "1:name": "name",
                  "2:login": "login"
                },
                "dataType": "issues"

              }
            ]
          }
        ]
      }
    ],
    "mappings": {
      "organizations": {
        "login": {
          "mapping": {
            "destination": "org_name",
            "primaryKey": true
          }
        },
        "id": {
          "mapping": {
            "destination": "org_id"

          }
        }
      },
        "repos": {
