{
  "baseUrl": "https://slack.com/api/",
  "authentication": {
    "type": "oauth20",
    "format": "json",
    "query": {
      "token": {
        "authorization": "data.access_token"
      }
    }
  },
  "http": {
    "headers": {
      "Accept": "application/json"
    },
    "defaultOptions": {
      "params": {
        "count": "1000"
      }
    }
  },
  "pagination": {
    "method": "response.param",
    "responseParam": "messages.999.ts",
    "queryParam": "latest"
  }
}
